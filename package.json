  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npx snowpack",
    "uninstall": "npx snowpack",
    "serve": "serve",
    "dev": "concurrently --kill-others \"npm run watch:ts\" \"npm run watch:sass:vendors\" \"npm run watch:sass:app\" \"npm run serve\"",
